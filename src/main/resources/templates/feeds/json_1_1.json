{
    "version": "https://jsonfeed.org/version/1.1",
    "title": "{{v feed_title}}{{/v}}",
    "home_page_url": "{{v feed_link}}{{/v}}",
    "description": "{{v feed_description}}{{/v}}",
    "authors": [
        {
            "name": "{{v feed_author}}{{/v}}"
        }
    ],
    "language": "{{v feed_language}}{{/v}}",
    "items": [{{v entries}}{{/v}}{{b entry}}
        {
            "id": "{{v entry_id}}{{/v}}",
            "url": "{{v entry_link}}{{/v}}",
            "tags": [{{v entry_category_section}}{{/v}}{{b entry_category}}"{{v entry_category}}{{/v}}"{{/b}}{{b entry_category_separator}}, {{/b}}],
            "title": "{{v entry_title}}{{/v}}",
            "content_html": "{{v entry_content}}{{/v}}",
            "date_published": "{{v entry_publishedDate}}{{/v}}",
            "authors": [
                {
                    "name": "{{v entry_author}}{{/v}}"
                }
            ]{{v image}}{{/v}}{{b image}},
            "image": "{{v entry_image}}{{/v}}"{{/b}}
        }{{b entry_separator}},{{/b}}{{/b}}
    ]
}